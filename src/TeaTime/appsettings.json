{
  "allowedHosts": "*",

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/teatime.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  "slack": {
    "verificationToken": ""
  },

  "mysql": {
    "host": "localhost",
    "password": "password",
    "username": "root"
  }
}
