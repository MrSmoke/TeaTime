{
  "allowedHosts": "*",

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/teatime.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  "slack": {
    "verificationToken": ""
  },

  "connectionStrings": {
    "mysql": "host=localhost;userid=root;password=password;database=teatime;"
  }
}
