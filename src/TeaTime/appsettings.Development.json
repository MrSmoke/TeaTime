{
  "serilog": {
    "minimumLevel": "Debug",
    "writeTo": [
      { "name": "Console" },
      {
        "name": "File",
        "args": {
          "path": "logs/teatime.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  }
}
